properties([
    
    parameters([
        choice(choices: ["QA", "Stage","Prod"].join("\n"), description: 'select state', name: 'Enviornment')
        ])
        
 ])
	



node {  

    stage('Build') { 
        echo "Buld the code"
	echo "code build is done"
	
	 echo "${params.Enviornment}"
    }
    stage('Test') { 
        
	echo "verify the URL"
    }
    stage('Deploy') { 
        echo "Deploy the code to QA"
    }
}

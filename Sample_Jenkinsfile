pipeline {
    agent any
    
    parameters {
  string defaultValue: 'sam', description: 'Enter UserName', name: 'uname'
  choice choices: ['Dev', 'QA', 'PROD'], description: 'Enter Env ', name: 'Env'
}

    stages {
        stage ('Initialize') {
            steps {
                echo  "Initializing the Code File"
                
                echo "${params.uname}"
                
                echo JOB_NAME
                
                echo BUILD_NUMBER
                
                echo WORKSPACE
                
                
            }
        }

        stage ('Build') {
            steps {
                echo 'Hello World'
            }
        }

         stage ('Deploy') {
            steps {
                echo 'Deployed an Artifact'
            }
        }
        
         stage ('Test') {
            steps {
                echo 'Test the app'
            }
        }
    }
}
